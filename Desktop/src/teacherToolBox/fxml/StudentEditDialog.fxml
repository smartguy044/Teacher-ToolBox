<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import com.jfoenix.controls.JFXButton?>

<AnchorPane prefHeight="224.0" prefWidth="321.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="teacherToolBox.controller.StudentEditDialogController">
    <children>
        <GridPane prefHeight="164.0" prefWidth="301.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                  AnchorPane.topAnchor="10.0">
            <children>
                <Label text="Student ID" GridPane.rowIndex="1"/>
                <Label text="First Name" GridPane.rowIndex="2"/>
                <Label text="Last Name" GridPane.rowIndex="3"/>
                <Label text="Gender" GridPane.rowIndex="4"/>
                <TextField fx:id="studentIDField" GridPane.columnIndex="1" GridPane.rowIndex="1" disable="true"/>
                <TextField fx:id="firstNameField" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <TextField fx:id="lastNameField" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <TextField fx:id="genderField" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
            </children>
            <columnConstraints>
                <ColumnConstraints fillWidth="true" hgrow="SOMETIMES" maxWidth="80.0" minWidth="80.0" prefWidth="80.0"/>
                <ColumnConstraints fillWidth="true" hgrow="SOMETIMES" maxWidth="231.0" minWidth="10.0"
                                   prefWidth="216.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
        <HBox id="HBox" alignment="CENTER" minWidth="-1.0" spacing="5.0" AnchorPane.bottomAnchor="10.0"
              AnchorPane.rightAnchor="10.0"/>
        <JFXButton buttonType="RAISED" onAction="#handleOk" layoutX="80.0" layoutY="175.0" mnemonicParsing="false"
                   prefWidth="80.0" style="-fx-text-fill:WHITE;-fx-background-color:#5264AE;-fx-font-size:14px;"
                   text="OK"/>
        <JFXButton buttonType="RAISED" onAction="#handleCancel" layoutX="181.0" layoutY="175.0" mnemonicParsing="false"
                   prefWidth="80.0" style="-fx-text-fill:WHITE;-fx-background-color:#5264AE;-fx-font-size:14px;"
                   text="Cancel"/>
    </children>
</AnchorPane>
